[DEFAULT]
# Options: PagerDuty, Slack, MS_Teams
service_choice = PagerDuty

[PagerDuty]
# Events API V2 is recommended for automated alerts
routing_key = "your_pagerduty_routing_key"
service_url = "https://events.pagerduty.com/v2/enqueue"

[Slack]
# The webhook URL for your Slack channel
webhook_url = "https://hooks.slack.com/services/T00000000/B00000000/XXXXXX"

[MS_Teams]
# The webhook URL for your MS Teams channel
webhook_url = "https://outlook.office.com/webhook/XXXXX/IncomingWebhook/XXXXX"

[Telegram]
# Your bot's unique token
bot_token = "YOUR_TELEGRAM_BOT_TOKEN"
# The ID of the chat where you want to send messages
chat_id = "YOUR_TELEGRAM_CHAT_ID"
